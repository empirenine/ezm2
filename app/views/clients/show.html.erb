<p id="notice"><%= notice %></p>

<button class="show-private-data" style="cursor: default;">
  Show private data?
</button>


<p>
  <b>First name:</b>
  <span class="private-data"><span><%= @client.first_name %></span></span>
</p>

<p>
  <b>Last name:</b>
  <span class="private-data"><span><%= @client.last_name %></span></span>
</p>

<p>
  <b>Identifier:</b>
  <%= @client.identifier %>
</p>

<p>
  <b>Client email:</b>
  <span class="private-data"><span><%= @client.client_email %></span></span>
</p>

<p>
  <b>Date of birth:</b>
  <%= @client.date_of_birth %>
</p>

<p>
  <b>Address:</b>
  <span class="private-data"><span><%= @client.address %></span></span>
</p>

<p>
  <b>Gender:</b>
  <%= @client.gender %>
</p>

<p>
  <b>Primary phone:</b>
  <span class="private-data"><span><%= @client.primary_phone %></span></span>
</p>

<p>
  <b>Secondary phone:</b>
  <span class="private-data"><span><%= @client.secondary_phone %></span></span>
</p>

<p>
  <b>Notes:</b>
  <%= @client.notes %>
</p>


<%= link_to 'Edit Client Information', edit_client_path(@client) %>
